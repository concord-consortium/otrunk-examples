                <OTUDLSection local_id="section-stems-roots-leaves" viewContainerIsUpdateable="true" name="Stems, roots, leaves" isComputerBased="true" isIntroduction="false" isMath="false" isPosttest="false" isPretest="false" isReading="false" isWrapup="false">
                  <content>
                    <OTCardContainer id="9b7e306f-7121-11dc-b75e-4b06cf421f29" contentsMayScroll="true" loadViewOnlyOnDemand="true" closeViewOnExit="true">
                      <cards>
                        <OTUDLCompoundDoc local_id="stems-roots-leaves-page-1" name="9b7e095e-7121-11dc-b75e-4b06cf421f29">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <div class="subtitle">Discovery question</div>
                                  Why do plants have stems, roots, and leaves?
                                  <p />
                                  <img src="http://udl.concord.org/artwork/plant_34/parts_of_plant/pl_34_parts_of_plant.png" />
                                  <p />
                                  In this activity, your job is to figure out why plants have leaves, stems, and roots.
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc local_id="stems-roots-leaves-page-2" name="4d68ebce-7131-11dc-b75e-4b06cf421f29">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <div class="subtitle">Engage</div>
                                  <table>
                                    <tr>
                                      <td>
                                        <img src="http://confluence.concord.org/download/thumbnails/12521/potato.jpg" />
                                      </td>
                                      <td>
                                        <img src="http://confluence.concord.org/download/thumbnails/12521/meadowgrass.jpg" />
                                      </td>
                                      <td>
                                        <img src="http://confluence.concord.org/download/thumbnails/12521/tree2.jpg" />
                                      </td>
                                    </tr>
                                  </table>
                                  <BR />
                                  <BR />
                                  What do these three pictures of plants have in common?
                                  <BR />
                                  <BR />
                                  <object refid="60846e5f-7131-11dc-b75e-4b06cf421f29" />
                                  <BR />
                                  <BR />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTText id="60846e5f-7131-11dc-b75e-4b06cf421f29" name="Text field" />
                          </documentRefs>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc local_id="stems-roots-leaves-page-3">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <div class="subtitle">Materials</div>
                                  <ul>
                                    <li>newspaper to cover tables</li>
                                    <li>house plants</li>
                                    <li>hand lens (Leaves and Roots experts only)</li>
                                    <li>scissors (Stem experts only)</li>
                                    <li>celery stalk (optional for Stem experts only)</li>
                                    <li>300 ml clear plastic cup or beaker (10 oz) (Root experts only)</li>
                                    <li>240 ml water (8 oz) (Root experts only)</li>
                                  </ul>
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc local_id="stems-roots-leaves-page-4">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  Find two people you want to work with in a group. Your teacher may help you pick your teammates and provide you with the materials.
                                  <BR />
                                  <BR />
                                  Each of you will investigate a separate part of the plant.  As a group, decide who will become the
                                  <b>Leaf expert</b>
                                  , the
                                  <b>Stem expert</b>
                                  ,
                                  and the
                                  <b>Root expert</b>
                                  .
                                  <BR />
                                  <BR />
                                  <table>
                                    <tr>
                                      <td>
                                        If you are the
                                        <b>Leaf expert</b>
                                      </td>
                                      <td>
                                        <object refid="${leaf-expert-button}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        If you are the
                                        <b>Stem expert</b>
                                      </td>
                                      <td>
                                        <object refid="${stem-expert-button}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        If you are the
                                        <b>Root expert</b>
                                      </td>
                                      <td>
                                        <object refid="${root-expert-button}" />
                                      </td>
                                    </tr>
                                  </table>
                                  <object refid="${leaves-section}" />
                                  <object refid="${path-selection-script}" />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTButton local_id="leaf-expert-button" text="click here!" />
                            <OTButton local_id="stem-expert-button" text="click here!" />
                            <OTButton local_id="root-expert-button" text="click here!" />
                            <object refid="${leaves-section}" />
                            <OTScriptObject local_id="path-selection-script">
                              <variables>
                                <OTScriptVariableComponent name="button1">
                                  <reference>
                                    <object refid="${leaf-expert-button}" />
                                  </reference>
                                </OTScriptVariableComponent>
                                <OTScriptVariableComponent name="button2">
                                  <reference>
                                    <object refid="${stem-expert-button}" />
                                  </reference>
                                </OTScriptVariableComponent>
                                <OTScriptVariableComponent name="button3">
                                  <reference>
                                    <object refid="${root-expert-button}" />
                                  </reference>
                                </OTScriptVariableComponent>
                                <OTScriptVariable name="replacementObject1">
