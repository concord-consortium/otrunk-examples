				        <OTUDLSection local_id="leaves-section" viewContainerIsUpdateable="true" name="Stems, roots, leaves" isComputerBased="true" isIntroduction="false" isMath="false" isPosttest="false" isPretest="false" isReading="false" isWrapup="false">
				          <content>
				            <OTCardContainer local_id="leaves-card-container" contentsMayScroll="true" loadViewOnlyOnDemand="true">
				              <cards>
				                <object refid="${stems-roots-leaves-page-1}" />
				                <object refid="${stems-roots-leaves-page-2}" />
				                <object refid="${stems-roots-leaves-page-3}" />
				                <object refid="${stems-roots-leaves-page-4}" />
				                <OTUDLCompoundDoc local_id="leaves-page-3">
				                  <bodyText>
				                    <div class="buffer">
				                      <div class="border">
				                        <div class="body">
				                          <div class="subtitle">Explore Leaves</div>
				                          Make observations about your plant by making the following questions.
				                          <BR />
				                          <BR />
				                          How many leaves are on the plant?
				                          <BR />
				                          <object refid="dd0d6e71-8366-11dc-8d72-7d9f1944a673" />
				                          <BR />
				                          <BR />
				                          Are all of the leaves the same size?
				                          <BR />
				                          <object refid="883c7ace-8366-11dc-8d72-7d9f1944a673" />
				                          <BR />
				                          <BR />
				                          Where are the biggest leaves found on the plant?
				                          <BR />
				                          <object refid="e3a49882-8366-11dc-8d72-7d9f1944a673" />
				                          <BR />
				                          <BR />
				                          Where are the smallest leaves found on the plant?
				                          <BR />
				                          <object refid="eabb2bc3-8366-11dc-8d72-7d9f1944a673" />
				                          <BR />
				                          <BR />
				                        </div>
				                      </div>
				                    </div>
				                  </bodyText>
				                  <documentRefs>
				                    <OTText id="dd0d6e71-8366-11dc-8d72-7d9f1944a673" name="Text field" />
				                    <OTChoice id="883c7ace-8366-11dc-8d72-7d9f1944a673" name="Choices">
				                      <choices>
				                        <OTCompoundDoc name="yes">
				                          <bodyText>yes</bodyText>
				                        </OTCompoundDoc>
				                        <OTCompoundDoc name="no">
				                          <bodyText>no</bodyText>
				                        </OTCompoundDoc>
				                      </choices>
				                    </OTChoice>
				                    <OTText id="e3a49882-8366-11dc-8d72-7d9f1944a673" name="Text field" />
				                    <OTText id="eabb2bc3-8366-11dc-8d72-7d9f1944a673" name="Text field" />
				                  </documentRefs>
				                </OTUDLCompoundDoc>
				                <OTUDLCompoundDoc local_id="leaves-page-4">
				                  <bodyText>
				                    <div class="buffer">
				                      <div class="border">
				                        <div class="body">
				                          <div class="subtitle">Explore Leaves page 2</div>
				                          Using a pair of scissors, cut off one of the biggest leaves of the plant. Make sure to pick it where it attaches to the stem as shown below.
				                          <BR />
				                          <p />
				                          <img src="http://confluence.concord.org/download/attachments/12521/clipping_leaf_small.jpg" />
				                          <p />
				                          Draw the leaf you picked and make sure to include the details in your drawing like color, shape, and size. Label any interesting features.
				                          <BR />
				                          <BR />
				                          <object refid="cc8a7444-836a-11dc-8d72-7d9f1944a673" />
				                          <BR />
				                          <BR />
				                          Leaves are generally green. The leaf captures the energy from the sun.
				                          <BR />
				                          <BR />
				                        </div>
				                      </div>
				                    </div>
				                  </bodyText>
				                  <documentRefs>
				                    <OTDrawingTool2 id="cc8a7444-836a-11dc-8d72-7d9f1944a673" name="Draw tool">
				                      <graphables>
				                        <OTDrawingShape type="" fillType="clear" x="0.0" y="0.0" width="0.0" height="0.0" color="0x000000" />
				                      </graphables>
				                    </OTDrawingTool2>
				                  </documentRefs>
				                </OTUDLCompoundDoc>
				                <OTUDLCompoundDoc id="c1e8f664-865e-11dc-a2ac-bf90d5b1c581" name="%c1e8f664-865e-11dc-a2ac-bf90d5b1c581">
				                  <bodyText>
				                    <div class="buffer">
				                      <div class="border">
				                        <div class="body">
				                          <div class="subtitle">Explore Leaves page 3</div>
				                          Use a hand lens to look at top side of your leaf. Describe and record what you see.
				                          <BR />
				                          <object refid="7ac1fe50-836d-11dc-8a7b-9f9002ae967c" />
				                          <BR />
				                          <BR />
				                          Use a hand lens to look at bottom side of your leaf. Describe and record what you see.
				                          <BR />
				                          <object refid="8fcef0a1-836d-11dc-8a7b-9f9002ae967c" />
				                          <BR />
				                          <BR />
				                          Air is made of many different gases. One of them is called carbon dioxide. Plants need carbon dioxide. It is one of the materials they take in to make food. Carbon dioxide enters the plant through a hole on the underside of the leaf. This hole is called the stomata. Plants take in carbon dioxide and let go of oxygen through their stomata.
				                          <BR />
				                          <BR />
				                          <img src="http://teemss2.concord.org/artwork/Teemss2_images/Units/u08_i01/stoma/U8stoma.png" />
				                        </div>
				                      </div>
				                    </div>
				                  </bodyText>
				                  <documentRefs>
				                    <OTText id="7ac1fe50-836d-11dc-8a7b-9f9002ae967c" name="Text field" />
				                    <OTText id="8fcef0a1-836d-11dc-8a7b-9f9002ae967c" name="Text field" />
				                  </documentRefs>
				                </OTUDLCompoundDoc>
				                <OTUDLCompoundDoc id="350cf9c5-865f-11dc-a2ac-bf90d5b1c581" name="%350cf9c5-865f-11dc-a2ac-bf90d5b1c581">
				                  <bodyText>
				                    <div class="buffer">
				                      <div class="border">
				                        <div class="body">
				                          <div class="subtitle">Explain Leaves</div>
				                          Below is the drawing of your leaf.
				                          <BR />
				                          <BR />
				                          <object refid="cc8a7444-836a-11dc-8d72-7d9f1944a673" />
				                          <BR />
				                          <BR />
				                          Below is another plant. Use your plant drawing to help answer the questions about this new plant.
				                          <BR />
				                          <BR />
				                          <img src="http://confluence.concord.org/download/attachments/12521/ipecacuanha.jpg" />
				                          <BR />
				                          <BR />
				                          <UL>
				                            <Li>Why are all the leaves on the outer part of the plant?</Li>
				                            <BR />
				                            <object refid="70aff12e-8670-11dc-97fd-673c6d6954f2" />
				                            <BR />
				                            <BR />
				                            <Li>What materials enter the plant through the leaves?</Li>
				                            <BR />
				                            <object refid="76215fef-8670-11dc-97fd-673c6d6954f2" />
				                            <BR />
				                            <BR />
				                            <Li>What materials leave the plant through the leaves?</Li>
				                            <BR />
				                            <object refid="79ff3660-8670-11dc-97fd-673c6d6954f2" />
				                          </UL>
				                          <BR />
				                          <BR />
				                        </div>
				                      </div>
				                    </div>
				                  </bodyText>
				                  <documentRefs>
				                    <object refid="cc8a7444-836a-11dc-8d72-7d9f1944a673" />
				                    <OTText id="70aff12e-8670-11dc-97fd-673c6d6954f2" name="Text field" />
				                    <OTText id="76215fef-8670-11dc-97fd-673c6d6954f2" name="Text field" />
				                    <OTText id="79ff3660-8670-11dc-97fd-673c6d6954f2" name="Text field" />
				                  </documentRefs>
				                </OTUDLCompoundDoc>
				                <object refid="${stems-roots-leaves-elaborate}"/>
				              </cards>
				              <currentCard>
				                <object refid="${leaves-page-3}" />
				              </currentCard>
				              <viewedCards>
				              	<object refid="${stems-roots-leaves-page-1}" />
				                <object refid="${stems-roots-leaves-page-2}" />
				                <object refid="${stems-roots-leaves-page-3}" />
				                <object refid="${stems-roots-leaves-page-4}" />
				              </viewedCards>
				            </OTCardContainer>
				          </content>
				          <header>
				            <OTUDLCompoundDoc id="2767fe20-6c7d-11dc-8314-0800200c9a66!/leaves-section/header">
				              <bodyText>
				                <div class="title">Leaf expert</div>
				              </bodyText>
				            </OTUDLCompoundDoc>
				          </header>
				          <footer>
				            <OTCompoundDoc id="2767fe20-6c7d-11dc-8314-0800200c9a66!/leaves-section/footer">
				              <bodyText>
				                <div align="center">
				                  <table>
				                    <tr>
				                      <td>
				                        <object refid="${section-leaves-menu}" viewid="${menu-horizontal-circle}" />
				                      </td>
				                    </tr>
				                  </table>
				                </div>
				              </bodyText>
				            </OTCompoundDoc>
				          </footer>
				          <north>
				            <object refid="2767fe20-6c7d-11dc-8314-0800200c9a66!/leaves-section/header" />
				          </north>
				          <center>
				            <object refid="${leaves-card-container}" />
				          </center>
