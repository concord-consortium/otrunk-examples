                  <header>
                    <OTUDLCompoundDoc id="2767fe20-6c7d-11dc-8314-0800200c9a66!/section_6/header" name="%15771a25-87cb-11dc-a57f-8bcc557bc796">
                      <bodyText>
                        <div class="title">Photosynthesis: A Closer Look</div>
                      </bodyText>
                    </OTUDLCompoundDoc>
                  </header>
                  <footer>
                    <OTUDLCompoundDoc id="2767fe20-6c7d-11dc-8314-0800200c9a66!/section_6/footer" name="%15771a26-87cb-11dc-a57f-8bcc557bc796">
