                                    <string>strong stem</string>
                                    <float>5.0</float>
                                    <float>29.21</float>
                                    <string>dark green</string>
                                    <string>30</string>
                                    <string>14</string>
                                    <string>strong stem</string>
                                    <float>6.0</float>
                                    <float>29.84</float>
                                    <string>dark green</string>
                                    <string>33</string>
