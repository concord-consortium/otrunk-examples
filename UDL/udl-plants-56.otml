                          </coachStatements>
                          <documentRefs>
                            <OTUDLQuestion id="32fbdfee-e16b-11dc-81e0-0b1cd468892e" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    Write a summary of Chapter 1.
                                    <br />
                                    <br />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
