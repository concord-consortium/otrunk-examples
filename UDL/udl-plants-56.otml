                            </OTDataTable>
                            <OTDataCollector local_id="line-graph-1" name="Data graph-1" title="Data graph">
                              <source>
                                <OTDataGraphable name="Plant 1" connectPoints="true" controllable="false" yColumn="1" drawMarks="false" color="16711680" xColumn="0" lineWidth="2.0">
                                  <dataStore>
                                    <object refid="${g1_datastore1}" />
                                  </dataStore>
                                </OTDataGraphable>
                              </source>
                              <xDataAxis>
                                <OTDataAxis min="0.0" max="12.0" label="Time" intervalWorld="1" units="Days" />
