                <OTUDLSection local_id="section_wrapup" name="Wrapping up">
                  <header>
                    <OTCompoundDoc>
                      <bodyText>
                        <div class="title">Wrapping up</div>
                      </bodyText>
                    </OTCompoundDoc>
                  </header>
                  <content>
                    <OTCardContainer local_id="exploration6-card-container" contentsMayScroll="true" loadViewOnlyOnDemand="true">
                      <cards>
                        <OTUDLCompoundDoc local_id="exploration6-1" name="1">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <div class="subtitle">Wrap-up</div>
                                  <object refid="${wrapup-ques1}" />
                                  <p />
                                  <object refid="${wrapup-ques2}" />
                                  <p />
                                  <object refid="${wrapup-ques3}" />
                                  <p />
                                  <object refid="${wrapup-ques4}" />
                                  <p />
                                  <object refid="${wrapup-ques5}" />
                                  <p />
                                  Let your teacher know when you have completed the friction unit. Your
																		teacher will then unlock the posttest for you.
                                  <p />
                                  <table>
                                    <tr>
                                      <td>Teacher password</td>
                                      <td>
                                        <object refid="${password-field}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        <object refid="${wrapup-submit-button}" />
                                      </td>
                                    </tr>
                                  </table>
                                  <object refid="${wrapup-button-script}" />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTQuestion local_id="wrapup-ques1">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="body-no-border">What is friction?</div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText rows="2" />
                              </input>
                            </OTQuestion>
                            <OTQuestion local_id="wrapup-ques2">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="body-no-border">Which world you you like best?</div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice>
                                  <choices>
                                    <OTCompoundDoc>
                                      <bodyText>
                                        <div class="body-no-border">Slippery world (almost no friction)</div>
                                      </bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc>
                                      <bodyText>
                                        <div class="body-no-border">Sticky world (lots of friction)</div>
                                      </bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc>
                                      <bodyText>
                                        <div class="body-no-border">Normal world (in between)</div>
                                      </bodyText>
                                    </OTCompoundDoc>
                                  </choices>
                                </OTChoice>
                              </input>
                            </OTQuestion>
                            <OTQuestion local_id="wrapup-ques3">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="body-no-border">Why?</div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText rows="2" />
                              </input>
                            </OTQuestion>
                            <OTQuestion local_id="wrapup-ques4">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="body-no-border">What do you think makes a surface have more or less friction?</div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText rows="2" />
                              </input>
                            </OTQuestion>
                            <OTQuestion local_id="wrapup-ques5">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="body-no-border">What would reduce the friction between two surfaces?</div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText rows="2" />
                              </input>
                            </OTQuestion>
                            <OTText local_id="password-field" isPassword="true" name="PasswordField" />
                            <OTText local_id="password" text="friction" name="Password" />
                            <OTButton local_id="wrapup-submit-button" name="Button" text="Submit" />
                            <OTScriptObject local_id="wrapup-button-script">
                              <variables>
                                <OTScriptVariable name="cardContainer">
                                  <reference>
                                    <object refid="${section-card-container}" />
                                  </reference>
                                </OTScriptVariable>
                                <OTScriptVariableComponent name="button">
                                  <reference>
                                    <object refid="${wrapup-submit-button}" />
                                  </reference>
                                </OTScriptVariableComponent>
                                <OTScriptVariable name="card">
                                  <reference>
                                    <object refid="${section_posttest}" />
                                  </reference>
                                </OTScriptVariable>
                                <OTScriptVariableComponent name="passwordfield">
                                  <reference>
                                    <object refid="${password-field}" />
