                            </div>
                          </bodyText>
                          <coachStatements>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_strategic}" />
                              </coach>
                              <statement>
                                <OTCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
