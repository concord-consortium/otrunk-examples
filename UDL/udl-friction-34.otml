                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_recognition}" />
                              </coach>
                              <statement>
                                <OTCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
