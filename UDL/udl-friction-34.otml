                <OTUDLSection local_id="section_exploration2" name="Playground surfaces" isComputerBased="true">
                  <header>
                    <OTCompoundDoc id="678b5190-0bcf-012a-b542-0017f2cc694e!/section_exploration2/header" showEditBar="false">
                      <bodyText>
                        <div class="title">What happens when you change the surface of a playground slide?</div>
                      </bodyText>
                    </OTCompoundDoc>
                  </header>
                  <content>
                    <OTCardContainer local_id="exploration3-card-container" contentsMayScroll="true" loadViewOnlyOnDemand="true" closeViewOnExit="false">
                      <cards>
                        <OTUDLCompoundDoc local_id="exploration3-1" name="engage" showDefinitions="true">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle">Discovery question</div>
                                      </td>
                                      <td align="right">
                                        <div class="page-number">1</div>
                                      </td>
                                    </tr>
                                  </table>
                                  What happens when you change the surface of a playground slide?
                                  <br />
                                  <br />
                                  <blockquote>
                                    <img src="http://udl.concord.org/artwork/friction_34/eduardo_sliding/fr_34_eduardo_sliding.png" border="1" />
                                  </blockquote>
                                  <br />
                                  In this activity, you will compare different surface materials for a 
          slide.
                                  <br />
                                  <br />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <coachStatements>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_affective}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Slides are fun!   Imagine if you were asked to design one.  Think about the different kinds of surfaces you might use.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_strategic}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">We will need to think about which materials would be good for a slide.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_recognition}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">We will try out different materials in a computer model and see which makes the best slide.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                          </coachStatements>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc local_id="exploration3-2" name="explore1" showDefinitions="true">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle">Engage</div>
                                      </td>
                                      <td align="right">
                                        <div class="page-number">2</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <div class="story-border">
                                    <div class="story">When they returned from Sticky World and Slippery World, Maria 
              and Eduardo wanted to explain to their friends what it was like. 
              They decided to use a computer model where they could change the 
              amount of friction.</div>
                                  </div>
                                  <br />
                                  In this model you can change how steep the slide is. You can also 
          change the surface of the slide. You have four choices of material:
                                  <br />
                                  <br />
                                  Steel:
                                  <img src="http://udl.concord.org/artwork/friction_34/steel_sample/steel_sample.png" />
                                  Wood:
                                  <img src="http://udl.concord.org/artwork/friction_34/wood_sample/wood_sample.png" />
                                  Ice:
                                  <img src="http://udl.concord.org/artwork/friction_34/ice_sample/ice_sample.png" />
                                  Carpet:
                                  <img src="http://udl.concord.org/artwork/friction_34/carpet_sample/carpet_sample.png" />
                                  <p>
                                    <br />
                                    <br />
                                    <object refid="3c0291b1-e3d6-11dc-9c4e-875b8e86b4b9" />
                                  </p>
                                  <br />
                                  <br />
                                  <object refid="4916f53e-e3d6-11dc-9c4e-875b8e86b4b9" />
                                  <br />
                                  <br />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTUDLQuestion id="3c0291b1-e3d6-11dc-9c4e-875b8e86b4b9" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>Which surface would be the most like Sticky World (the greatest friction)?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                            <OTUDLQuestion id="4916f53e-e3d6-11dc-9c4e-875b8e86b4b9" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>Which surface would be most like Slippery World (the least friction)?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
                          <coachStatements>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_affective}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Think about what each of these materials feels like.  Maybe you have a steel bucket, wheelbarrow, or dog dish at home.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_strategic}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Each material feels different.  Now you need to think about putting them in order from the most sticky to the most slippery.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_recognition}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Ordering the materials helps you to think about them in relationship to one another.  You will then be able to design the best slide.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                          </coachStatements>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc local_id="pgsurfaces_engage_2" name="PS-Engage-2" showDefinitions="true">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle-continued" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">3</div>
                                      </td>
                                    </tr>
                                  </table>
                                  Write in the boxes below the surface materials (steel, wood, ice, 
          carpet) in order from the most sticky to the most slippery.
                                  <br />
                                  <br />
                                  <table>
                                    <tr>
                                      <td>More friction (most sticky)</td>
                                    </tr>
                                    <tr>
                                      <td>
                                        <object refid="${act2-exp-text1}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        <object refid="${act2-exp-text2}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        <object refid="${act2-exp-text3}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        <object refid="${act2-exp-text4}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>Less friction (most slippery)</td>
                                    </tr>
                                  </table>
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTText local_id="act2-exp-text1" />
                            <OTText local_id="act2-exp-text2" />
                            <OTText local_id="act2-exp-text3" />
                            <OTText local_id="act2-exp-text4" />
                          </documentRefs>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc local_id="exploration3-3" name="explore2" showDefinitions="true">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle">Explore</div>
                                      </td>
                                      <td align="right">
                                        <div class="page-number">4</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <table>
                                    <tr>
                                      <td colspan="2">
                                        <object refid="${act2-exp2-fr_model}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        <object refid="${act2-exp2-fr1_snapshot_button}" />
                                      </td>
                                      <td />
                                    </tr>
                                  </table>
                                  <p />
                                  In the model above, the surface material of the slide is set to be 
          steel.
                                  <ol>
                                    <li>
                                      Click the
                                      <b>Play</b>
                                      button and lift the slide by clicking the
                                      <b>Raise 
              ramp</b>
                                      button. Wait a couple of seconds after clicking the 
              button to see if the boy starts to slide. If he does not move, 
              click
                                      <b>Raise ramp</b>
                                      again. Count how many times you clicked 
              the
                                      <b>Raise ramp</b>
                                      button before the boy started to slide.
                                      <p />
                                    </li>
                                    <li>
                                      Use the Reset button to test the steel slide material again.
                                      <p />
                                    </li>
                                    <li>
                                      Record your results of the height needed to start the boy to 
              slide in the table below.
                                      <br />
                                      <table border="1">
                                        <tr>
                                          <td />
                                          <td>Number of clicks</td>
                                        </tr>
                                        <tr>
                                          <td>Steel</td>
                                          <td>
                                            <object refid="${act2-exp2-text1}" />
                                          </td>
                                        </tr>
                                      </table>
                                    </li>
                                  </ol>
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTFrictionModel local_id="act2-exp2-fr_model" startMaterial="steel" showGraph="false" showPushButtons="false" showSlider="false" rampAngle="0" />
                            <OTSnapshotButton local_id="act2-exp2-fr1_snapshot_button">
                              <target>
                                <object refid="${act2-exp2-fr_model}" />
                              </target>
                              <snapshotAlbum>
                                <object refid="${album}" />
                              </snapshotAlbum>
                            </OTSnapshotButton>
                            <OTText local_id="act2-exp2-text1" />
                          </documentRefs>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc local_id="pgsurface_explore_2" name="explore2" showDefinitions="true">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">5</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <table>
                                    <tr>
                                      <td colspan="2">
                                        <object refid="${act2-exp3-fr_model}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        <object refid="${act2-exp3-fr1_snapshot_button}" />
                                      </td>
                                      <td />
                                    </tr>
                                  </table>
                                  <p />
