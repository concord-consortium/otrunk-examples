                                      </td>
                                      <td>
                                        <object refid="${act3-exp1-text6}" viewid="${text-label}" />
                                      </td>
                                      <td>
                                        <object refid="${act3-exp2-text2}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        <object refid="${act3-exp1-text9}" viewid="${text-label}" />
                                      </td>
                                      <td>
                                        <object refid="${act3-exp1-text10}" viewid="${text-label}" />
                                      </td>
                                      <td>
                                        <object refid="${act3-exp2-text3}" />
                                      </td>
                                    </tr>
                                    <tr>
                                      <td>
                                        <object refid="${act3-exp1-text13}" viewid="${text-label}" />
                                      </td>
                                      <td>
