                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Think about Sticky World and Slippery World.  Why would you want to leave them and return to Normal World?</div>
                                      </div>
                                    </div>
                                    <img src="http://udl.concord.org/share/shared-images/clunk-anim.gif" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_recognition}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">I would want to get back to Normal World where I don't move too fast or too slow.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
