                          <documentRefs>
                            <OTSoundPlayer id="a0ad5465-f061-11dc-ad95-89456d96baaf" name="Sound player" src="http://udl.concord.org/sound/friction_34/fr_34_ch4_p7.mp3" />
                            <OTUDLQuestion id="62d2aa97-e3c8-11dc-ac60-9facaf86308a" name="Question" showConvertButton="true">
