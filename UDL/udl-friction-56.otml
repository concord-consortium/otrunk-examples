                                  <scaffolding>
                                    <OTUDLCompoundDoc>
                                      <bodyText />
                                    </OTUDLCompoundDoc>
                                  </scaffolding>
                                </OTQuestion>
                                <OTQuestion name="L4">
                                  <prompt>
                                    <OTUDLCompoundDoc>
                                      <bodyText>If there were no friction, would the penny heat up?</bodyText>
                                    </OTUDLCompoundDoc>
                                  </prompt>
