                            <object refid="${page-rule}" />
                          </menuRule>
                        </OTUDLMenu>
                        <object refid="${menu-horizontal-circle}" />
                      </documentRefs>
                    </OTUDLCompoundDoc>
                  </footer>
                  <north>
                    <object refid="1f2503dc-775f-11dc-88e0-f91e4efaa30a" />
                  </north>
