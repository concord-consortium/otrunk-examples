                        <OTUDLCompoundDoc name="%497bdc85-768e-11dc-a568-7984efa6d7c4">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <div class="subtitle">Materials</div>
                                  <ul>
                                    <li>Two plastic bowls filled with sand</li>
                                    <li>Water</li>
                                    <li>Incandescent lamp</li>
                                    <li>Plastic wrap</li>
                                    <li>Relative humidity sensor (optional)</li>
