                                              <bodyText>rain and snow</bodyText>
                                            </OTCompoundDoc>
                                            <OTCompoundDoc name="oceans">
                                              <bodyText>oceans</bodyText>
                                            </OTCompoundDoc>
                                          </choices>
                                        </OTChoice>
                                      </documentRefs>
                                    </OTUDLCompoundDoc>
                                  </input>
                                  <scaffolding>
