                                </OTCompoundDoc>
                                <OTCompoundDoc name="Evaporation.">
                                  <bodyText>Evaporation.</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </documentRefs>
                        </OTUDLCompoundDoc>
