                    <OTUDLCompoundDoc id="4cb57e9d-7764-11dc-bcdb-d9cc05265d45" name="%4cb57e9d-7764-11dc-bcdb-d9cc05265d45">
                      <bodyText>
                        <div class="title">Light and heat</div>
                      </bodyText>
                    </OTUDLCompoundDoc>
