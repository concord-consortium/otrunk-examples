                                        </OTSensorDataProxy>
                                      </dataProducer>
                                      <dataStore>
                                        <OTDataStore dt="0.1" numberChannels="1">
                                          <channelDescriptions>
                                            <OTDataChannelDescription name="dt" precision="2" numericData="true" />
                                            <OTDataChannelDescription name="" unit="V" precision="-2" numericData="true" />
                                          </channelDescriptions>
                                        </OTDataStore>
                                      </dataStore>
                                    </OTDataGraphable>
                                    <OTDataGraphable drawMarks="false" color="65280" connectPoints="true" xColumn="0" yColumn="1" name="Fourth battery" lineWidth="2.0">
                                      <dataProducer>
                                        <OTSensorDataProxy>
                                          <request>
                                            <object refid="16780d50-cada-11dc-9f17-f581dffccb2a" />
                                          </request>
                                        </OTSensorDataProxy>
                                      </dataProducer>
                                      <dataStore>
                                        <OTDataStore dt="0.1" numberChannels="1">
