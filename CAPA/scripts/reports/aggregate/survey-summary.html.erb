<hr />

<div id="content">
  <h3><%= $title.getText %></h3>
  <table class="a" cellpadding="5px">
    <% for user in users %>
      <tr class="a">
  		<td class="a"><%= user.getName %></td>
  		<% for question in questions %>
  		<td class="a"><%= surveyAnswerText(user, question) %></td>
  		<% end %>
      </tr>
    <% end %>
  </table>
</div>