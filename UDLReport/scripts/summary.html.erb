<div id="content">
  
  <h2>Report for <%= activityTitle %></h2>

  <table>
    <tr>
     <th>User</th>
     <% activitySections.each do |section| %>
      <th><%= linkToObject section.name, section, $section_view %></th>  
     <% end %>
    </tr>
    <% users.each do |user| %>
      <tr>
        <td><%= linkToObject user.name, user, $user_view %></td>
       <% userVisitedSections = visitedSections(user) %>
       <% activitySections.each do |section| %>
        <% userSection = userObject(section, user) %>
        <td>
          <% if userVisitedSections.include? userSection %>
            <img src="../UDL/images/tick.png"/>
          <% end %>
        </td>  
       <% end %>        
      </tr>
    <% end %>    
  </table>
   
  <%# Find all the sections inside of that container and list them here %>
  
  
</div>