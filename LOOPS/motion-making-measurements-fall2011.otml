                        <OTOverlayWrapper id="859dd10c-617e-11df-90e6-cf681ddd0d2d" name="GiantStepIntersession" embedWrappedObject="false">
                          <wrappedObject>
                            <OTObjectSet name="GiantStepIntersession (Set)">
                              <objects>
                                <object refid="57d7ad45-617e-11df-90e6-cf681ddd0d2d" />
