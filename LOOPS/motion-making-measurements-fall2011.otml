                        <OTQuestion id="f67de6c2-e5fb-11e0-b841-e0cb4ebd2b6d" name="HowLongQ">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>How long was the duration between the two jumps?  How did you get your answer?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTText />
                          </input>
                        </OTQuestion>
