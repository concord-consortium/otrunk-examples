                                <OTCompoundDoc id="f7377574-e5fb-11e0-b2b3-e0cb4ebd2b6d" name="2">
                                  <bodyText>2</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="f7377574-e5fb-11e0-b2b4-e0cb4ebd2b6d" name="7">
                                  <bodyText>7</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="f7377574-e5fb-11e0-b2b5-e0cb4ebd2b6d" name="-8">
                                  <bodyText>-8</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="f7377574-e5fb-11e0-b2b6-e0cb4ebd2b6d" name="2">
                                  <bodyText>8</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                          <correctAnswer>
