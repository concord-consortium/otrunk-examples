      <root>
        <OTCurriculumUnit version="2" name="LOOPS Activity">
          <activity>
            <OTSection local_id="section_1" name="Modeling Motion" headerVisible="false">
              <header>
                <OTCompoundDoc showEditBar="false">
                  <bodyText>
                    <div class="title">Modeling Motion</div>
